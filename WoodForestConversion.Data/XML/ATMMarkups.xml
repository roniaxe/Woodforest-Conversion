<?xml version="1.0" encoding="utf-8" ?>
<sqlinstruction>
	<!--  Debit Card Pending Transactions  (Build ATM Systems and Markups)
	-->
	<event server="#server_activitydb#" database="#database_atm#" timeout="2700">
		<executesql>EXEC dbo.spATMMarkups</executesql>
	</event>
	<event server="#server_activitydb#" database="#database_activity#" timeout="300">
		<executesql>EXEC HostMerge_RefreshMarkupAmounts</executesql>
	</event>
</sqlinstruction>