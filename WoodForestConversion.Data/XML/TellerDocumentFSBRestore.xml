<?xml version="1.0" encoding="utf-8" ?>
<sqlinstruction>
	<!--  
	This is to Restore the Documents tables that become corrupted in TellerPlus FSB - this sp restores them
	-->
	<event server="TellerDB" database="TellerWNBUtils" timeout="300">
		<executesql>
			EXEC dbo.DocumentFSBRestore 1
		</executesql>
	</event>
</sqlinstruction>
