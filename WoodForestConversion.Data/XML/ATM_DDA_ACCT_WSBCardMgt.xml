<?xml version="1.0" encoding="utf-8" ?>
<import name="ATM_DDA_ACCT_MAP">
	<queues total="5" bulksize="2000" errorthreshold="255" />
	<source location="#import_extractto#\atm_dda_acct" isxml="0" delimiter="|" queuefirst="0" rootpath="" />
	<destination server="PaymentServicesDB" database="#database_staging#" entity="ATMCONTROL_DDA_ACCT_WSB" 
		truncate="1" dropindexes="1" user="#import_user#" password="#import_password#" />
	<validation check="#import_validation#" />
	<mappings>
		<map index="0" column="CardNumber" filecolumn="REC.ID" ignore="0" expression="^ATM([0-9]*)$" group="1" />
		<map index="0" column="Z_ID" filecolumn="REC.ID" ignore="0" />
		<map index="1" column="DDA_ACCT" filecolumn="DDA ACCT#" ignore="0" />
		<map index="2" column="Z_ASSOC_ROW" filecolumn="Z_ASSOC" ignore="0" />
	</mappings>
</import>
