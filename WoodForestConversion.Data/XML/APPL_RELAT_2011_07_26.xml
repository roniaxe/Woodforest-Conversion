<?xml version="1.0" encoding="utf-8" ?>
<import name="APPL_RELAT_MAP">
	<queues total="5" bulksize="3000" errorthreshold="255" />
	<source location="#import_extractto#\appl_relat" isxml="0" delimiter="|" queuefirst="0" rootpath="" />
	<destination server="#server_banclinedb#" database="#database_bancline#" entity="APPLPTR_RELAT_ASSOC" 
		truncate="1" dropindexes="1" user="#import_user#" password="#import_password#" />
	<validation check="#import_validation#" />
	<mappings>
		<map index="0" column="Z_ID" filecolumn="REC.ID" ignore="0" />
		<map index="1" column="RELAT_DESC" filecolumn="RELATIONSHIP DESCRIPTION" ignore="0" />
		<map index="2" column="RELAT_CODE" filecolumn="RELATED CODE" ignore="0" />
		<map index="3" column="RELAT_SEQ" filecolumn="RELATED SEQ NO" ignore="0" />
		<map index="4" column="RELAT_TAXID" filecolumn="RELATED TAX ID" ignore="0" />
		<map index="5" column="RELAT_CIF" filecolumn="RELATED CIF" ignore="0" />
		<map index="6" column="RELAT_ADDR_PTR" filecolumn="RELAT ADDR PTR" ignore="0" />
		<map index="7" column="RELAT_IND_LIA_TYP" filecolumn="REL LIAB TYP" ignore="0" />
		<map index="8" column="RELAT_IND_LIA_VAL" filecolumn="REL.LIAB VAL" ignore="0" />
		<map index="9" column="Z_ASSOC_ROW" filecolumn="Z_ASSOC" ignore="0" />
	</mappings>
</import>
