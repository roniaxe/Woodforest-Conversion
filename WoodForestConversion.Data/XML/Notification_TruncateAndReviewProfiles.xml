<?xml version="1.0" encoding="utf-8" ?>
<sqlinstruction>
	<!-- Truncate XML prebuild tables -->
	<event server="#server_woodforestdb#" database="Woodforest" >
		<executesql>
			TRUNCATE TABLE dbo.Notification_XmlAccount;
			TRUNCATE TABLE dbo.Notification_XmlTransaction;
		</executesql>
	</event>
	<!-- Review the profiles in the system and determine which should be sent today. -->
	<event server="#server_woodforestdb#" database="Woodforest" timeout="1200">
		<executesql>
			EXEC dbo.Notification_Engine_ReviewActiveProfiles;
		</executesql>
	</event>
</sqlinstruction>