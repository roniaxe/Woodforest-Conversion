<?xml version="1.0" encoding="utf-8" ?>
<import name="APPL2_MAP">
	<queues total="5" bulksize="3000" errorthreshold="255" />
	<source location="#import_extractto#\appl2" isxml="0" delimiter="|" queuefirst="0" rootpath="" />
