		truncate="1" dropindexes="1" user="#import_user#" password="#import_password#" />
	<mappings>
		<map index="0" column="ACCT" filecolumn="REC.ID" ignore="0" expression="^(DDA|ATM|CD|CLN|CLC|SDB|CRR)([0-9]*)$" group="2" />
